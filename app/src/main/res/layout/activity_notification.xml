<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".presentation.notification.NotificationActivity">

    <include
        android:id="@+id/include_app_bar_notification"
        layout="@layout/include_app_bar_middle"/>

    <include
        android:id="@+id/include_empty_notification"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        layout="@layout/include_empty_notification"
        app:layout_constraintTop_toBottomOf="@id/include_app_bar_notification"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>